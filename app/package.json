{
  "name": "mindknight-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "debug": "cross-env node --experimental-specifier-resolution=node --loader ./loader.js ./src/_debugger/debugger.ts",
    "migrate": "cross-env node --experimental-specifier-resolution=node --loader ./loader.js ./prisma/migrations/calculate_elo_all.ts",
    "transfer": "cross-env node --experimental-specifier-resolution=node --loader ./loader.js ./src/_uploader/uploader.ts",
    "patch": "npx next-ws-cli@latest patch",
    "dev": "DOTENV_KEY=$(npx dotenv-vault keys development) next dev",
    "dev:prod": "DOTENV_KEY=$(npx dotenv-vault keys production) next dev",
    "dev:windows": "set /p DOTENV_KEY=<dek && cross-env START_BROWSER=true && next dev",
    "dev:linux": "DOTENV_KEY=$(cat ./dek) next dev",
    "build:windows": "set /p DOTENV_KEY=<dek && cross-env BUILD=true next build",
    "start:windows": "set /p DOTENV_KEY=<dek && cross-env START_BROWSER=true next start",
    "build": "yarn build:linux",
    "build:linux": "DOTENV_KEY=$(cat ./dek) next build",
    "start:linux": "DOTENV_KEY=$(cat ./dek) next start",
    "lint": "next lint",
    "add-translation": "ts-node src/translations/scripts/addTranslation.ts",
    "find-unused-translations": "ts-node src/translations/scripts/findUnused.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.1",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.13",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-data-grid": "^7.1.1",
    "@tanstack/react-query": "^5.28.6",
    "@tanstack/react-query-devtools": "^5.28.6",
    "axios": "^1.6.8",
    "cross-env": "^7.0.3",
    "dom-to-image": "^2.6.0",
    "dotenv": "^16.4.5",
    "eslint-config-next": "^14.2.3",
    "gif.js.optimized": "^1.0.1",
    "lodash": "^4.17.21",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "next-auth-steam": "^0.3.0",
    "next-ws": "^1.0.1",
    "node-machine-id": "^1.1.12",
    "nuqs": "^1.17.1",
    "open": "^10.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-drag-drop-files": "^2.3.10",
    "react-highlight-words": "^0.20.0",
    "sharp": "^0.33.3",
    "sillyname": "^0.1.0",
    "steam-client": "^2.5.8",
    "tail": "^2.2.6",
    "ws": "^8.16.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@prisma/client": "^5.13.0",
    "@types/dom-to-image": "^2.6.7",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20",
    "@types/passport-steam": "^1.0.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-highlight-words": "^0.16.7",
    "@types/steam-client": "^2.5.6",
    "@types/tail": "^2.2.3",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "prisma": "^5.13.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
